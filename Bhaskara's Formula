#include<bits/stdc++.h>
using namespace std;
int main()
{
   double a,b,c,cal;
   cin>>a>>b>>c;
   cal=(b*b)-(4*a*c);
   if(cal<0||a==0)
   {
    cout<<"Impossivel calcular"<<endl;
   }
else {

    double r1,r2;
    r1=(-b+sqrt(cal))/(2*a);
    r2=(-b-sqrt(cal))/(2*a);
    cout<<fixed<<setprecision(5)<<"R1 = "<<r1<<endl<<"R1 = "<<r2<<endl;
}
    return 0;
}
